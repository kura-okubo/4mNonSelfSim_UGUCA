# geometry and discretization
x_length = 0.25
z_length = 0.25
nb_x_elements = 128
nb_z_elements = 128

# external loading
normal_load = 1e6

# top material properties
material top [
  E = 3e9
  nu = 0.25
  rho = 1500
]

# interface properties
interface cohesion [
  tauc = 1.2e6
  dc = 2e-5
]

# nucleation
a0 = 0.05

# duration and time stepping
duration = 4.5e-4
tsf = 0.3 # time step factor

# dumping
dump_fields = cohesion,top_disp
nb_dumps = 60